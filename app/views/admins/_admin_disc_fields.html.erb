<div class="nested-fields">
  <div class="field">
    <p>ディスク名</p>
    <%= f.text_field :name, class: "form-control", placeholder: 'ディスク名を入力してください' %>
  </div>
  <div class="links">
    <%= link_to_remove_association "ディスクを削除", f %>
  </div>
    <%= f.fields_for :songs do |song| %>
    <%= render 'admin_song_fields', f: song %>
    <% end %>
  <div class="links">
    <%= link_to_add_association '曲を追加', f, :songs %>
  </div>
</div>