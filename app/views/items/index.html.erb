


<h1>商品一覧</h1>




	<div class="container">
	 <div class="row">
<% @items.each do |item| %>
	  <div class="col-md-4">
	   <%= link_to item_path(item) do %>
	    <%= attachment_image_tag '#', :image %>

			<div><%= item.name %></div>
			<div><%= item_artists.name %></div>
			<div><%= item.price %></div>



	   <% end %>
	  </div>
	 </div>
	</div>
<div>
	<%= paginate(@itemse, :outer_window => 3) %>
</div>
<% end %>
